{
    "name": "danrfiuza/sayhithere",
    "description": "A simple library created to practice publishing packages with composer",
    "require": {
        "php": ">=7.3"
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "type": "library",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name":"Daniel Fiuza",
            "email": "daniel.fiuza@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"Fiuza\\App\\":"src/Fiuza/App"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Fiuza\\App\\": "tests"
        }
    },
    "config":{
        "bin-dir":"bin/"
    }
}
